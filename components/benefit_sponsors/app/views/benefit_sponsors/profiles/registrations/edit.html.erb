<% profile_type = @agency.organization.profile.profile_type %>
<% if is_broker_profile?(profile_type) %>
  <%= render 'shared/error_messages', :object => @agency %>
  <%= render partial: 'benefit_sponsors/shared/profiles/broker_agency/menu', :locals => { :broker_profile_id => @agency.organization.profile.id, :active_tab => "home-tab" } %>
  <%= form_for @agency, as: :agency, url: profiles_registration_path(id: @agency.profile_id), method: :put do |f| %>
    <%= render "benefit_sponsors/profiles/registrations/broker_agency_form", f: f %>
  <% end %>
<% elsif is_sponsor_profile?(profile_type) %>
  <% content_for :navigation do %>
    <%= render :partial => 'benefit_sponsors/profiles/employers/employer_profiles/primary_nav', :locals => {:active_tab=>params[:tab], organization: @agency.organization, profile: @agency.organization.profile} %>
  <% end %>
  <%= form_for @agency, as: :agency, url: profiles_registration_path(id: @agency.profile_id), method: :put do |f| %>
    <%= render "benefit_sponsors/profiles/employers/employer_profiles/employer_form", f: f %>
  <% end %>
<% end %>