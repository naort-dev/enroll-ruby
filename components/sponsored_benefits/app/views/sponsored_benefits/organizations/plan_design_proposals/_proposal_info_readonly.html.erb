<div class="row no-buffer row-form-wrapper">
  <div class="col-md-4 col-md-4 col-xs-12 form-group form-group-lg no-pd">
    <h4>Quote Name</h4>
    <%= @plan_design_proposal.title %>
  </div>

  <div class="col-md-4 col-md-4 col-xs-12 form-group form-group-lg no-pd">
    <h4>Effective Date</h4>
    <%= @plan_design_proposal.effective_date.strftime("%m/%d/%Y") %>
  </div>

  <div class="col-md-4 col-md-4 col-xs-12 form-group form-group-lg no-pd">
    <h4>Quote Date</h4>
    <%= @plan_design_proposal.updated_at.strftime("%m/%d/%Y") %>
  </div>

  <br />

  <div class="col-md-4 col-md-4 col-xs-12 form-group form-group-lg no-pd">
    <h4>SIC Code</h4>
    <%= @plan_design_organization.sic_code %>
  </div>

  <div class="col-md-4 col-md-4 col-xs-12 form-group form-group-lg no-pd">
    <h4>Zip Code</h4>
    <%= @plan_design_organization.office_locations.first.address.zip %>
  </div>

  <div class="col-md-4 col-md-4 col-xs-12 form-group form-group-lg no-pd">
    <h4>County</h4>
    <%= @plan_design_organization.office_locations.first.address.county %>
  </div>
</div>