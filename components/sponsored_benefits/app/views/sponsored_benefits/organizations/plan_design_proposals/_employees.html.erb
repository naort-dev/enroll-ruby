<div class="row">
  <div class="col-xs-12 add-new-employees" >
    <%= link_to "<i class='fa fa-file-text-o' aria-hidden='true'></i> Upload Employee Roster".html_safe, new_census_members_plan_design_census_employee_path(proposal_id: @plan_design_proposal.proposal.id, modal: 'upload'), remote: 'true', class: "btn btn-default" %>
    <%#= link_to "<i class='fa fa-cloud-download' aria-hidden='true'></i> Download Employee Roster".html_safe, employers_employer_profile_export_census_employees_path(@employer_profile, format: :csv), class: "btn btn-default" %>
    <%= link_to "<i class='fa fa-user-plus' aria-hidden='true'></i> Add Employee".html_safe, new_census_members_plan_design_census_employee_path(proposal_id: @plan_design_proposal.proposal.id), :remote => true, class: "btn btn-default pull-right" %>
  </div>
</div>

<div id="new_employee_form">
</div>

<div class="row">
  <div class="col-xs-12" >
    <div class= "effectivedatatable">
      <%= raw render_datatable(@datatable) %>
    </div>
  </div>
</div>

<script type="text/javascript" charset="utf-8">
    initializeDataTables();
    $('.buttons-csv').hide()
    $('.buttons-excel').hide()
</script>

<%= paginate @census_employees, param_name: :pagina, remote: true if params[:page].present? %>

<div class="row">
  <div class="col-md-9">
    <%# if (@total_census_employees_quantity.present? || !params[:page].nil?) && (@total_census_employees_quantity != @census_employees.count) %>
        <%#= render 'shared/alph_paginate', url: employers_employer_profile_path(@employer_profile), alphs: @page_alphabets, remote: true, status: status, full_index_url: employers_employer_profile_path(@employer_profile, status: status, employee_name: params[:employee_name]) %>
    <%# end %>
  </div>
  <div class="col-md-3 text-right content-bottom">
    <h4><%#= "#{@census_employees.count} out of #{@total_census_employees_quantity}" if @total_census_employees_quantity.present? %></h4>
  </div>
</div>