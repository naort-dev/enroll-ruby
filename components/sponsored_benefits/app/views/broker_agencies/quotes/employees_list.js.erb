		$('#family-container').html("<%= j render(partial: 'employees_list', locals:{employee: @employee_present} ) %>");
<% else %>
	'<%= form_for @quote, url: {action: "my_quotes"}, method: "put" do |f| %>'
		$('#family-container').html("<%= escape_javascript(render( 'employees_list' )) %>");
	'<% end %>'
<% end %>
$('.dropdown-toggle').dropdown();
