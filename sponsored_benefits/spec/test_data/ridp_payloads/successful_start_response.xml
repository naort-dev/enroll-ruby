<ridp:interactive_verification_result
  xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
  xmlns:ridp='http://openhbx.org/api/terms/1.0'
  xsi:schemaLocation='http://openhbx.org/api/terms/1.0 file:/Users/tevans/proj/cv_example_builder/verification_services.xsd'>
  <ridp:session>
    <ridp:response_code>urn:openhbx:terms:v1:interactive_identity_verification#MORE_INFORMATION_REQUIRED</ridp:response_code>
    <ridp:transaction_id>transaction id for reference</ridp:transaction_id>
    <ridp:session_id>session id for reference</ridp:session_id>
    <ridp:question>
      <ridp:question_id>First Question</ridp:question_id>
      <ridp:question_text>If you had to answer a question</ridp:question_text>
      <ridp:response_options>
        <ridp:response_option>
          <ridp:response_id>A</ridp:response_id>
          <ridp:response_text>pick answer A</ridp:response_text>
        </ridp:response_option>
        <ridp:response_option>
          <ridp:response_id>B</ridp:response_id>
          <ridp:response_text>pick answer B</ridp:response_text>
        </ridp:response_option>
      </ridp:response_options>
    </ridp:question>
    <ridp:question>
      <ridp:question_id>Second Question</ridp:question_id>
      <ridp:question_text>If somehow there was another question</ridp:question_text>
      <ridp:response_options>
        <ridp:response_option>
          <ridp:response_id>C</ridp:response_id>
          <ridp:response_text>pick answer C</ridp:response_text>
        </ridp:response_option>
        <ridp:response_option>
          <ridp:response_id>D</ridp:response_id>
          <ridp:response_text>pick answer D</ridp:response_text>
        </ridp:response_option>
      </ridp:response_options>
    </ridp:question>
  </ridp:session>
</ridp:interactive_verification_result>
